{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [ // Pastikan ada 3 entry di sini
    {
      "src": "/socket.io",
      "dest": "server.js"
    },
    {
      "src": "/keep-alive",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}